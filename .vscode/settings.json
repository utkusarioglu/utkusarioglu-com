{
  "runOnSave.commands": [
    {
      "match": "\\.env$",
      "async": true,
      "command": "/scripts/create-env-example.sh"
    },
    {
      "match": "\\.env$",
      "async": true,
      "command": "gh secret set -f .env"
    }
  ],
  "yaml.schemas": {
    "https://json.schemastore.org/helmfile.json": "**/*.helm"
  },
}
